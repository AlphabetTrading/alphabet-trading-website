    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "formik": "^2.2.9",
    "framer-motion": "^8.5.2",
    "handlebars": "^4.7.7",
    "next": "13.1.1",
    "nodemailer": "^6.9.0",
